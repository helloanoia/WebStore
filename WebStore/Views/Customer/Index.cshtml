@model IEnumerable<WebStore.ViewModels.EmployeeView>
@{
    ViewData["Title"] = "Список сотрудников";
}
<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
    <table>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(item.FirstName, "Details", new { id = item.Id })
                </td>
                <td>@item.SurName</td>
                <td>@item.Patronymic</td>
                <td>@item.Age</td>
            </tr>
        }
    </table>
</body>
</html>